# PUBLIC: Example environment variables for the Code Review Frontend.
# Copy this file to .env and set the values accordingly. Do NOT commit your real .env.

# PUBLIC_INTERFACE
# Base URL of your deployed site (used for GitHub OAuth redirect)
REACT_APP_SITE_URL=http://localhost:3000

# PUBLIC_INTERFACE
# GitHub OAuth Client ID (from your GitHub OAuth app)
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id_here

# PUBLIC_INTERFACE
# Optional: GitHub OAuth scopes (comma separated or space separated)
REACT_APP_GITHUB_SCOPES=read:user repo

# PUBLIC_INTERFACE
# Optional: Backend URL (if you use a backend to exchange the OAuth code for access tokens)
# If not available, the app will only perform the front-end redirect and display demo data
REACT_APP_BACKEND_URL=http://localhost:8000
